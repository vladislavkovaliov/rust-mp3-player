export component EllipsisText inherits Rectangle {
    in property <string> text: "";

    width: 344px - 96px;
    clip: true;
    Text {
        width: parent.width;
        height: parent.height;
        vertical-alignment: center;
        horizontal-alignment: left;
        overflow: elide;
        text: root.text;
        font-size: 14px;
        color: black;
    }
}
